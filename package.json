{
  "name": "pilot",
  "version": "0.1.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "turbo": "turbo"
  },
  "packageManager": "pnpm@10.26.1",
  "private": true,
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.11",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "packageExtensions": {
      "@hookform/resolvers@*": {
        "peerDependencies": {
          "zod": "*"
        }
      }
    },
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2",
      "require-in-the-middle": "^8.0.1",
      "import-in-the-middle": "^2.0.6"
    }
  }
}
